int count = 0;
int counter = 0;
double total = 0;

print("Number count:");
count = input(0);

while(counter < count)
  {
    double number = 0;
    print("Number:");
    number = input(number);
    counter = counter + 1;
    total = total + number;
  }
print("The average of ",count, " numbers is: ",total / count);