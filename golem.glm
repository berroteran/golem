int cantidad = 0;
int contador = 0;
double total = 0;

print("Cantidad de numeros:");
cantidad = input(0);

while(contador < cantidad)
  {
    double monto = 0;
    print("monto:");
    monto = input(monto);
    contador = contador + 1;
    total = total + monto;
  }
print("El promedio de los ",cantidad, " es ",total / cantidad);