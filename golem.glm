sharedobject "libgtk-3.so.0"

/*
 *  Hello Word
 */


extends GApplication
{
  extern int run(int argc,pointer argv);
}

int main(int argc,pointer argv)
{
  
  GtkApplication app = new GtkApplication {
    "application-id": "org.gnome.desktop.golem"
  };
  
  app.on("activate", do () {
      print("Hello Word");
  });
  
  app.run(argc,argv);
  return 0;
}